{
  "name": "csp-create",
  "productName": "csp-create",
  "version": "1.0.0",
  "description": "My Electron application description",
  "author": "Spaceface16518 <33424649+Spaceface16518@users.noreply.github.com>",
  "main": "dist/main.js",
  "scripts": {
    "start": "node .",
    "test": "node .",
    "prestart": "yarn run build",
    "pretest": "yarn run build",
    "build": "",
    "lint": "node_modules/.bin/eslint ."
  },
  "keywords": [],
  "license": "MIT",
  "dependencies": {
    "electron": "^1.8.4",
    "fs": "^0.0.1-security",
    "jquery": "^3.3.1",
    "path": "^0.12.7"
  },
  "babel": {
    "presets": [
      ["env", {
        "targets": {
          "node": "current"
        }
      }]
    ]
  },
  "eslintConfig": {
    "extends": "google",
    "env": {
      "es6": true,
      "jquery": true,
      "node": true,
      "browser": true
    },
    "parserOptions": {
      "ecmaVersion": 2015,
      "sourceType": "module",
      "ecmaFeatures": {
        "arrowFunctions": true,
        "impliedStrict": false,
        "modules": true
      }
    },
    "rules": {
      "require-jsdoc": "off",
      "comma-dangle": "off",
      "quotes": "warn"
    }
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "browserify": "^16.1.1",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "fakefile": "^0.0.9",
    "uglify": "^0.1.5"
  }
